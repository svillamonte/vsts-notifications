{
    "manifest_version": 2,
    "name": "VSTS Notifications",
    "version": "0.1",
    "icons": {
        "128": "assets/app_icon_128.png"
    },
    "permissions": [
        "alarms", 
        "notifications", 
        "https://timely.visualstudio.com/*"
        ],
    "app": {
        "background": {
            "scripts": ["background.js"],
            "persistent": false
        }
    },
    "minimum_chrome_version": "48"
}